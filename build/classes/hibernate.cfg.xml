<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/farmacotecnia?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password"></property>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/MaterialDeEnvasado.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/ProductosUnion.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/MaterialParaElaborar.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/Etiquetas.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/MaterialParaElaborarUnion.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/MaterialDeEnvasadoUnion.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/Maquinaria.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/MaquinariaUnion.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/FichasDeMedicamentos.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/Reenvasados.hbm.xml"/>
    <mapping resource="es/sacyl/caza/farmacia/modelo/clases/Productos.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
